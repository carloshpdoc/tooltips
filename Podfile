  pod 'Quick', '4.0.0'
end

target 'HintsOnboarding' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for HintsOnboarding

  target 'HintsOnboardingTests' do
    # Pods for testing
    inherit! :complete
    test_pods
  end

end
